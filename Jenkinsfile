pipeline {
    agent any
    stages {
        stage('Fetch code') {
            steps {
                sh 'mvn --version'
            }
        }
         stage('build code') {
            steps {
                sh 'ls'
            }
        }
        stage('run test and publish test result') {
            steps {
                sh 'ls'
            }
        }
     
         stage('deploy war files') {
            steps {
                sh 'ls'
            }
        }
    }
}
